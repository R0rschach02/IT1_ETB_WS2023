#include <stdio.h>	// Standard Eingabe/Ausgabe Funktionen

#include <time.h>

int main(void)
{
	unsigned short wert;

	unsigned short i;
	printf("Quadratzahlen Tabelle \n");

	printf("Eingabe Anzahl Eintraege: ");

	scanf_s("%hu", &wert);

	for (i = 1; i <= wert; i++)
	{
		
		printf("%10hu %10hu\n", i, i*i);

	}


	return 0;
}
