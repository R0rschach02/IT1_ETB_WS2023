#include <stdio.h>	// Standard Eingabe/Ausgabe Funktionen

#include <time.h>

int main(void)
{
	unsigned short wert = 10; 

	scanf_s("%hu", &wert);

	printf("Quadratzahlen Tabelle \n");

	printf("Eingabe Anzahl Eintraege: %hu", wert); 

	for (unsigned short i = 0; i >= wert; i++)
	{
		wert* wert;
		printf("%hu \tt")

	}

	return 0;
}
